{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport 'chartjs-adapter-date-fns';\nimport { ref, onMounted, nextTick, onUnmounted } from 'vue';\nimport axios from 'axios';\nimport { drawChart } from '../../common/common.js';\nimport moment from 'moment';\nexport default {\n  name: 'TimeChart',\n  setup() {\n    const date = new Date();\n    const startDate = ref(new Date(date.getFullYear(), date.getMonth(), 1));\n    const endDate = ref(new Date());\n    const isLoading = ref(false);\n    const chartCanvasRefs = ref([]);\n    const charts = ref([]);\n    let currentPage = 1;\n    const pageSize = 3;\n    const handleDateChange = (event, dateRef) => {\n      const dateString = event.target.value;\n      if (!dateString) return;\n      const [year, month, day] = dateString.split('-').map(Number);\n      dateRef.value = new Date(year, month - 1, day);\n    };\n    const handleStartDateChange = event => handleDateChange(event, startDate);\n    const handleEndDateChange = event => handleDateChange(event, endDate);\n    const getData = async () => {\n      const formattedStartDate = moment(startDate.value).format('YYYY-MM-DD');\n      const formattedEndDate = moment(endDate.value).format('YYYY-MM-DD');\n      if (formattedStartDate > formattedEndDate) {\n        alert(\"시작날짜는 끝날짜보다 작아야 합니다.\");\n        return;\n      }\n      isLoading.value = true;\n      try {\n        const response = await axios.get('/get/chartData', {\n          params: {\n            startDate: formattedStartDate,\n            endDate: formattedEndDate,\n            page: currentPage,\n            pageSize: pageSize\n          }\n        });\n        const data = response.data;\n        const time = data.map(item => new Date(item.time).getTime());\n        const ping = data.map(item => item.ping);\n        const serverName = data.map(item => item.serverName);\n        charts.value = Array.from({\n          length: 5\n        }, () => ({\n          time: [...time],\n          ping: [...ping],\n          serverName: serverName[0] // Assuming all server names are the same for simplification\n        }));\n        await nextTick();\n        currentPage++;\n        await drawChartsInGroups(1);\n      } catch (error) {\n        console.error('Error fetching data:', error);\n      } finally {\n        isLoading.value = false;\n      }\n    };\n    const drawChartAsync = (context, time, ping) => {\n      return new Promise(resolve => {\n        requestAnimationFrame(() => {\n          drawChart(context, time, ping);\n          resolve();\n        });\n      });\n    };\n    const drawChartsInGroups = async groupSize => {\n      for (let i = 0; i < charts.value.length; i += groupSize) {\n        const groupPromises = [];\n        for (let j = 0; j < groupSize && i + j < charts.value.length; j++) {\n          const index = i + j;\n          const chart = charts.value[index];\n          const canvas = chartCanvasRefs.value[index];\n          if (canvas) {\n            const context = canvas.getContext('2d');\n            groupPromises.push(drawChartAsync(context, chart.time, chart.ping));\n          }\n        }\n        await Promise.all(groupPromises);\n        await new Promise(resolve => setTimeout(resolve, 100));\n      }\n    };\n    let lastScrollPosition = 0; // 이전 스크롤 위치를 저장할 변수\n\n    const handleVisibilityChange = async () => {\n      let documentHeight = document.documentElement.scrollHeight;\n      let scrollHeight = window.scrollY;\n      let windowHeight = window.innerHeight;\n\n      // 스크롤이 아래로 내려갔고, 이전 스크롤 위치와 현재 스크롤 위치가 다른 경우에만 실행\n      if (scrollHeight >= documentHeight - windowHeight - 100 && scrollHeight !== lastScrollPosition) {\n        await nextTick();\n        await getData();\n        // 기존의 차트 개수에 3을 더하여 새로운 차트를 추가\n        for (let i = 0; i < 3; i++) {\n          const newIndex = charts.value.length;\n          const newChart = {\n            time: [],\n            ping: [],\n            serverName: \"\" // 서버 이름 초기화\n          };\n          charts.value.push(newChart);\n          // 새로운 차트의 데이터를 가져오고 렌더링\n          const chartData = charts.value[newIndex];\n          const canvas = chartCanvasRefs.value[newIndex];\n          if (canvas) {\n            const context = canvas.getContext('2d');\n            await drawChartAsync(context, chartData.time, chartData.ping);\n          }\n        }\n        // 현재 스크롤 위치를 이전 스크롤 위치로 업데이트\n        lastScrollPosition = scrollHeight;\n        currentPage++;\n      }\n    };\n    const handleScroll = async () => {\n      let documentHeight = document.documentElement.scrollHeight;\n      let scrollHeight = window.scrollY;\n      let windowHeight = window.innerHeight;\n\n      // 스크롤이 화면 아래의 일정 위치(여기서는 100px)에 도달했을 때만 데이터를 추가로 가져옵니다.\n      if (scrollHeight >= documentHeight - windowHeight - 100) {\n        // 스크롤 위치가 이동할 때마다 스크롤 이벤트를 제거하여 중복 호출을 방지합니다.\n        document.removeEventListener('scroll', handleScroll);\n        await getData();\n        currentPage++; // 페이지 번호를 증가시킵니다.\n        // 스크롤 이벤트를 다시 추가합니다.\n        document.addEventListener('scroll', handleScroll);\n      }\n    };\n    onMounted(() => {\n      getData();\n      document.addEventListener('scroll', handleVisibilityChange, true);\n\n      //console.log(scrollBottom);\n    });\n    onUnmounted(() => {\n      document.removeEventListener('scroll', handleVisibilityChange, true);\n    });\n\n    // visibleCharts를 사용하여 현재 화면에 보이는 차트를 관리\n    //  const visibleCharts = ref([]);\n\n    // // visibleCharts를 업데이트하는 함수\n    // const updateVisibleCharts = () => {\n    //   visibleCharts.value = charts.value.slice(0, currentPage * pageSize);\n\n    // };\n\n    return {\n      startDate,\n      endDate,\n      handleStartDateChange,\n      handleEndDateChange,\n      getData,\n      isLoading,\n      chartCanvasRefs,\n      charts\n      //  updateVisibleCharts\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","nextTick","onUnmounted","axios","drawChart","moment","name","se